{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "EdwAdfInboundBlueRidge"
		},
		"LsAsaAmmexEdw_EdwAdfPipelineReadOnly_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsAsaAmmexEdw_EdwAdfPipelineReadOnly'"
		},
		"LsAsaAmmexEdw_EdwAdfPipelineWrite_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LsAsaAmmexEdw_EdwAdfPipelineWrite'"
		},
		"EdwAdfKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://EdwAdfKeyVault.vault.azure.net/"
		},
		"LsAdfEdwFTPBR_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.brinv.com"
		},
		"LsAdfEdwFTPBR_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Ammex_Prod"
		},
		"LsAdlEdwInboundBR_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://edwinbound.dfs.core.windows.net"
		},
		"LsAdlsAmmexEdw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ammexedwsynapsestorage.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "EdwAdfInboundBlueRidge_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LsAsaAmmexEdw_EdwAdfPipelineReadOnly_connectionString": {
						"value": "[parameters('LsAsaAmmexEdw_EdwAdfPipelineReadOnly_connectionString')]"
					},
					"LsAsaAmmexEdw_EdwAdfPipelineWrite_connectionString": {
						"value": "[parameters('LsAsaAmmexEdw_EdwAdfPipelineWrite_connectionString')]"
					},
					"EdwAdfKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('EdwAdfKeyVault_properties_typeProperties_baseUrl')]"
					},
					"LsAdfEdwFTPBR_properties_typeProperties_host": {
						"value": "[parameters('LsAdfEdwFTPBR_properties_typeProperties_host')]"
					},
					"LsAdfEdwFTPBR_properties_typeProperties_userName": {
						"value": "[parameters('LsAdfEdwFTPBR_properties_typeProperties_userName')]"
					},
					"LsAdlEdwInboundBR_properties_typeProperties_url": {
						"value": "[parameters('LsAdlEdwInboundBR_properties_typeProperties_url')]"
					},
					"LsAdlsAmmexEdw_properties_typeProperties_url": {
						"value": "[parameters('LsAdlsAmmexEdw_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}