{
	"name": "Ingest8x8",
	"properties": {
		"activities": [
			{
				"name": "GetSourceNetworkFolder",
				"type": "GetMetadata",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DsSourceNetworkPathCsv",
						"type": "DatasetReference",
						"parameters": {
							"SourceNetworkPath": {
								"value": "@pipeline().parameters.SourceNetworkPath",
								"type": "Expression"
							}
						}
					},
					"fieldList": [
						{
							"value": "@pipeline().parameters.SourceNetworkPathFileName",
							"type": "Expression"
						}
					],
					"storeSettings": {
						"type": "FtpReadSettings",
						"enablePartitionDiscovery": false,
						"useBinaryTransfer": false,
						"disableChunking": false
					},
					"formatSettings": {
						"type": "DelimitedTextReadSettings"
					}
				}
			}
		],
		"parameters": {
			"SourceNetworkPath": {
				"type": "string",
				"defaultValue": "ftp.smartfile.com"
			},
			"SourceNetworkPathFileName": {
				"type": "string",
				"defaultValue": "shares/CallDetailRecord"
			}
		},
		"folder": {
			"name": "1 - Ingest"
		},
		"annotations": []
	}
}