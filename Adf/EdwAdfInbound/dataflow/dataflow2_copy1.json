{
	"name": "dataflow2_copy1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DsAbsInboundEdw",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DsAsaAmmexEdw_EdwAdfPipelineWrite_Unpivot",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     skipLines: 8,",
				"     wildcardPaths:['*.csv']) ~> source1",
				"source1 unpivot(output(",
				"          {Period Shipped} as string,",
				"          {Sold Item} as string",
				"     ),",
				"     ungroupBy({Manufacturer #},",
				"          Article,",
				"          {Article Name},",
				"          SourceFileName,",
				"          PipelineName,",
				"          LoadDate,",
				"          FileLastModifiedDate,",
				"          IsSourceDeleted,",
				"          IsSourceDeletedDate),",
				"     lateral: false,",
				"     ignoreNullPivots: false) ~> unpivot1",
				"unpivot1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: false,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          {Manufacturer #},",
				"          Article,",
				"          {Article Name},",
				"          SourceFileName,",
				"          PipelineName,",
				"          LoadDate,",
				"          FileLastModifiedDate,",
				"          IsSourceDeleted,",
				"          IsSourceDeletedDate,",
				"          {Period Shipped},",
				"          {Sold Item}",
				"     )) ~> sink1"
			]
		}
	}
}