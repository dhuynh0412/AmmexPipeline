{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DsLsAdEdwTest",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "unpivot1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Manufacturer#} as string,",
				"          Article as string,",
				"          {Article Name} as string,",
				"          {_col3_} as string,",
				"          {_col4_} as string,",
				"          {_col5_} as string,",
				"          {_col6_} as string,",
				"          {_col7_} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     skipLines: 7) ~> source1",
				"source1 unpivot(output(",
				"          Period as string,",
				"          {_col3_} as string,",
				"          {_col4_} as string,",
				"          {_col5_} as string,",
				"          {_col6_} as string,",
				"          {_col7_} as string",
				"     ),",
				"     ungroupBy({Manufacturer#},",
				"          Article,",
				"          {Article Name}),",
				"     lateral: false,",
				"     ignoreNullPivots: false,",
				"     partitionBy('hash', 1)) ~> unpivot1"
			]
		}
	}
}