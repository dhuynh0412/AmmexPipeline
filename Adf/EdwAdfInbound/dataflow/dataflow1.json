{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DsLsAdEdwTest",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DsAsaAmmexEdw_EdwAdfPipelineWrite",
						"type": "DatasetReference"
					},
					"name": "Test"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'FileName',",
				"     wildcardPaths:['*.csv']) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {Location Type} as string,",
				"          Dept as string,",
				"          Class as string,",
				"          {SKU Number} as string,",
				"          {SKU Status} as string,",
				"          {Quantity On Hand} as string,",
				"          VPC as string,",
				"          UPC as string,",
				"          {Unit Sales} as string,",
				"          {Retail Sales} as string,",
				"          {Cost Sales} as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     staged: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> Test"
			]
		}
	}
}